{
  "versions": [
    {
      "name": "python",
      "cmd": "python --version 2>&1",
      "regex": "Python\\s+([0-9\\.]+)"
    },
    {
      "name": "git",
      "cmd": "git --version 2>&1",
      "regex": "version\\s+([0-9\\.]+)"
    }
  ],
  "cpus": [
    {
      "name": "cpu",
      "cmd": "top -bn1 | grep \"Cpu(s)\" | sed -E 's/.*\\s([0-9]*\\.[0-9]*)%id.*/\\1/' | awk '{print 100.0 - $1}'"
    }
  ]
}
